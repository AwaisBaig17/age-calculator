{
  "name": "@awaisbaig/age-calculator",
  "version": "1.0.2",
  "description": "Universal age calculator library for JavaScript/TypeScript - works with React, Angular, Vue, and all JS frameworks",
  "keywords": [
    "age-calculator",
    "date",
    "age",
    "calculator",
    "typescript",
    "esm",
    "react",
    "angular",
    "vue",
    "svelte",
    "framework-agnostic"
  ],
  "author": "Awais Baig <awaisbaig1711@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AwaisBaig17/age-calculator.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.2",
    "vitest": "^2.0.0",
    "tsup": "^8.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  }
}
